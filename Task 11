let a = Math.sign(+prompt(`enter number 1`));
let b = Math.sign(+prompt(`enter number 2`));
let c = Math.sign(+prompt(`enter number 3`));
if(a + b + c === 3 || a + b + c === -1) {
  console.log('+')
} else if(a + b + c === -3 || a + b + c === 1) {
  console.log('-')
} else if(a === 0 || b === 0 || c === 0) {
  console.log(`unsigned`)
} else{
  console.log(`Invalid`)
}
